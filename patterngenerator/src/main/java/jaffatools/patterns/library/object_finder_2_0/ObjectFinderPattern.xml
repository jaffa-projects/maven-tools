            <Template>patterns/library/object_finder_2_0/ComponentStrutsActionXML.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>StrutsConfigAction.xfragment</FileName>
            <Override>True</Override>
        </Build>
    
        <Build>
            <Template>patterns/library/object_finder_2_0/ComponentTilesDefinitions.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>ComponentTilesDefinitions.xfragment</FileName>
            <Override>False</Override>
        </Build>
    
        <Build>
            <Template>patterns/library/object_finder_2_0/ApplicationResources.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>ApplicationResources.properties</FileName>
            <Override>True</Override>
        </Build>
