            <Override>Merge</Override>
        </Build>
    
        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentController.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower).ui</Package>
            <FileName>$(Component.Upper1)Component.java</FileName>
            <Override>Merge</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentCriteriaForm.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower).ui</Package>
            <FileName>$(Component.Upper1)CriteriaForm.java</FileName>
            <Override>Merge</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentResultsForm.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower).ui</Package>
            <FileName>$(Component.Upper1)ResultsForm.java</FileName>
            <Override>Merge</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentCriteriaAction.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower).ui</Package>
            <FileName>$(Component.Upper1)CriteriaAction.java</FileName>
            <Override>Merge</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentResultsAction.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower).ui</Package>
            <FileName>$(Component.Upper1)ResultsAction.java</FileName>
            <Override>Merge</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentCriteriaJSP.wm</Template>
            <Package>html.$(Application.Lower).$(Module.Lower).$(Component.Lower).jsp</Package>
            <FileName>criteria.jsp</FileName>
            <Override>False</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentResultsJSP.wm</Template>
            <Package>html.$(Application.Lower).$(Module.Lower).$(Component.Lower).jsp</Package>
            <FileName>results.jsp</FileName>
            <Override>False</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentDefinitionXML.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>ComponentDefinition.xfragment</FileName>
            <Override>False</Override>
        </Build>

        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentStrutsFormBeanXML.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>StrutsConfigFormBean.xfragment</FileName>
