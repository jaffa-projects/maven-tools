        </Build>
    
        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentStrutsGlobalForwardXML.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>StrutsConfigGlobalForward.xfragment</FileName>
            <Override>True</Override>
        </Build>
    
        <Build>
            <Template>patterns/library/object_lookup_1_0/ComponentStrutsActionXML.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>StrutsConfigAction.xfragment</FileName>
            <Override>True</Override>
        </Build>
    
        <Build>
            <Template>patterns/library/object_lookup_1_0/ApplicationResources.wm</Template>
            <Package>java.$(BasePackage.Lower).components.$(Component.Lower)</Package>
            <FileName>ApplicationResources.properties</FileName>
